import sys
import math
from collections import deque


def task(s):
    s = s[::-1]

    cntr = [0, 0, 0, 0, 0]
    values = [1, 10, 100, 1000, 10000]
    ans = 0
    best = 0
    dp = [0, 0, 0, 0, 0]

    for c in s:
        j = ord(c) - 65
        cnt = 0
        last = -1

        for i in range(j + 1, 5):
            if cntr[i] > 0:
                last = i

            cnt += cntr[i]

        if cnt == 0:
            ans += values[j]
        elif cnt > 1:
            ans -= values[j]
        else:
            dp[j] += 1

    print(s)


t = int(input())
for _ in range(t):
    s = input()
    task(s)
