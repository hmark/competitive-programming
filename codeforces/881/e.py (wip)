import sys
import math
from collections import deque

input = sys.stdin.readline


def task(n, m, lrs, q, xs):
    # print(lrs)

    a = []
    for l in range(1, n + 1):
        if l in lrs:
            alen = len(a)
            for r in lrs[l]:
                a.append([l, r, math.floor((r - (l - 1)) / 2) + 1])
            lrs[l] = alen

    # cache = []
    # last = 0
    # for i in range(n + 1):
    #     if i in lrs:
    #         last = lrs[i]
    #     cache.append(last)

    # print(a)
    # print(cache)

    ans = 0
    for x in xs:
        ans += 1

        for lrs in a:
            if lrs[1] < x:
                continue
            elif lrs[0] <= x:
                lrs[2] -= 1
                if lrs[2] == 0:
                    print(ans)
                    return

    print(-1)


t = int(input())
for _ in range(t):
    n, m = map(int, input().split())
    lrs = dict()
    for i in range(m):
        l, r = map(int, input().split())
        if not l in lrs:
            lrs[l] = set()
        lrs[l].add(r)
    q = int(input())
    xs = []
    for i in range(q):
        x = int(input())
        xs.append(x)
    task(n, m, lrs, q, xs)
