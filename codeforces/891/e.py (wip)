import sys
import math
from collections import deque

input = sys.stdin.readline


def task(n, x):
    for i in range(n):
        x[i] = [x[i], i]
    x.sort(key=lambda x: x[0])
    mn = x[0][0]
    mx = x[n - 1][0]

    ans = []
    for i in range(n):
        ans.append(n * (mx - mn + 1))

    print(x, ans)

    cnt = 0
    total = 0
    for i in range(1, n):
        total += x[i][1]
        cnt += total * (x[i][0] - x[i - 1][0])
        ans[x[i][1]] -= cnt

    cnt = 0
    for i in reversed(range(0, n - 1)):
        total += x[i][1]
        cnt += total * (x[i][0] - x[i + 1][0])
        ans[x[i][1]] -= cnt

    print(ans)


t = int(input())
for _ in range(t):
    n = int(input())
    x = list(map(int, input().split()))
    task(n, x)
